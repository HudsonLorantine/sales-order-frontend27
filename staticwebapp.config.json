{
  "routes": [
    {
      "route": "/api/orders",
      "allowedRoles": ["anonymous"],
      "serve": "/api/orders.json",
      "statusCode": 200
    },
    {
      "route": "/api/customers",
      "allowedRoles": ["anonymous"],
      "serve": "/api/customers.json",
      "statusCode": 200
    },
    {
      "route": "/api/products",
      "allowedRoles": ["anonymous"],
      "serve": "/api/products.json",
      "statusCode": 200
    },
    {
      "route": "/api/orders/*",
      "allowedRoles": ["anonymous"],
      "serve": "/api/order-detail.json",
      "statusCode": 200
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"],
      "serve": "/api/default.json",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/api/*"]
  }
}
