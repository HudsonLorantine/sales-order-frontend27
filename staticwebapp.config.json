{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "http://sales-order-backend-v1-2.azurewebsites.net/api/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css"
  }
}
